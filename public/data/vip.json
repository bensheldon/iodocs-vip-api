                            "Type": "static",
                            "Description": "Leave this alone."
                        },
                        {
                            "Name": "$format",
                            "Required": "Y",
                            "Default": "json",
                            "Type": "enumerated",
                            "EnumeratedList": [
                                "json",
                                "xml"
                               ],
                            "Description": "Format of outputted data. (json or xml.)"
