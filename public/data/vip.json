                {
                    "MethodName": "getPollingLocation",
                    "Synopsis": "Get polling location by address.",
                    "HTTPMethod": "GET",
                    "URI": "/GetPollingLocations2",
                    "RequiresOAuth": "N",
                    "parameters": [
                        {
                            "Name": "house",
                            "Required": "Y",
                            "Default": "2167",
                            "Type": "integer",
                            "Description": "House number."
                        },
                        {
                            "Name": "street",
                            "Required": "Y",
                            "Default": "'W 30th St'",
                            "Type": "string",
                            "Description": "Street name. Wrap in single quotes."
                        },
                        {
                            "Name": "city",
                            "Required": "Y",
                            "Default": "'Cleveland'",
                            "Type": "string",
                            "Description": "City name. Wrap in single quotes."
                        },
                        {
                            "Name": "state",
                            "Required": "Y",
                            "Default": "'OH'",
                            "Type": "string",
                            "Description": "State abbreviation. Wrap in single quotes."
                        },
                        {
                            "Name": "zip",
                            "Required": "Y",
                            "Default": "'44113'",
                            "Type": "string",
                            "Description": "Zip code. Wrap in single quotes."
                        },
                        {
                            "Name": "$format",
                            "Required": "Y",
                            "Default": "json",
                            "Type": "enumerated",
                            "EnumeratedList": [
                                "json",
                                "xml"
                               ],
                            "Description": "Format of outputted data. (json or xml.)"
                        },
                        {
                            "Name": "$expand",
                            "Required": "Y",
                            "Default": "Election/State/ElectionAdministration,Locations/PollingLocation/Address,Locations/SourceStreetSegment/NonHouseAddress",
                            "Type": "static",
                            "Description": "Leave this alone."
                        },
                        {
                            "Name": "onlyupcoming",
                            "Required": "N",
                            "Default": "false",
                            "Type": "enumerated",
                            "EnumeratedList": [
                                "true",
                                "false"
                               ],
                            "Description": "Return information only for upcoming elections, if they exist."
                        }
                    ]
